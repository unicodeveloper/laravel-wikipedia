{
    "name": "unicodeveloper/laravel-wikipedia",
    "description": "Laravel Bridge for Wikipedia",
    "keywords": [
        "unicodeveloper",
        "laravel-wikipedia",
        "laravel",
        "wikipedia",
        "php"
    ],
    "homepage": "https://twitter.com/unicodeveloper",
    "license": "MIT",
    "authors": [
        {
            "name": "unicodeveloper",
            "email": "prosperotemuyiwa@gmail.com",
            "homepage": "https://twitter.com/unicodeveloper",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Unicodeveloper\\Larapedia\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unicodeveloper\\Larapedia\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
